   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "import statsmodels.formula.api as sm\n",
    "x = np.arange(100)\n",
    "y = 0.5*x -20 + np.random.randn(len(x))\n",
    "df = pd.DataFrame({'X': x , 'Y' : y})\n",
    "model = sm.ols('y~x', data=df).fit()\n",
    "print( model.summary() )"
   ]
  }
